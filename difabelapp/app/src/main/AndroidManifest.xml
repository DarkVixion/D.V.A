<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.difabelapp">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Testloginrdt">
        <activity
            android:name="com.example.difabelapp.Splashscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.difabelapp.MainActivity"></activity>
        <activity android:name="com.example.difabelapp.Register" />
        <activity android:name="com.example.difabelapp.Tts"/>
        <activity android:name="com.example.difabelapp.Admin"/>
        <activity android:name="com.example.difabelapp.Login"/>
        <activity android:name="com.example.difabelapp.Icontts"/>

    </application>

</manifest>